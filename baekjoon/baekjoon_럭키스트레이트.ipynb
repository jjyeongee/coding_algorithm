   "source": [
    "n = input()\n",
    "length = len(n)\n",
    "sum = 0\n",
    "\n",
    "for i in range(length // 2):\n",
    "    sum += int(n[i])\n",
    "\n",
    "for i in range(length // 2, length):\n",
    "    sum -= int(n[i])\n",
    "    \n",
    "if sum == 0:\n",
    "    print(\"LUCKY\")\n",
    "else:\n",
    "    print(\"READY\")"
