    "n = int(input())\n",
    "\n",
    "def hanoi(n, a, b, c):\n",
    "    if n == 1:\n",
    "        print(a, c)\n",
    "    else:\n",
    "        hanoi(n-1, a, c, b)\n",
    "        print(a, c)\n",
    "        hanoi(n-1, b, a, c)\n",
    "sum = 1\n",
    "for i in range(n-1):\n",
    "    sum = sum * 2 + 1\n",
    "print(sum)\n",
    "hanoi(n, 1, 2, 3)"
